            android:id="@+id/action_viewReservationFragment_to_startReservationFragment"
            app:destination="@id/startReservationFragment" />
        <action
            android:id="@+id/action_viewReservationFragment_to_startApplicationFragment"
            app:destination="@id/startApplicationFragment" />
    </fragment>
