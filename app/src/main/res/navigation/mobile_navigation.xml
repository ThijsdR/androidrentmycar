<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/mobile_navigation"
    app:startDestination="@id/startApplicationFragment">

    <fragment
        android:id="@+id/startApplicationFragment"
        android:name="com.example.proftaakandroid.ui.StartApplicationFragment"
        android:label="StartApplicationFragment" >
        <action
