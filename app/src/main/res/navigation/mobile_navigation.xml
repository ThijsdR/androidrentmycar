            android:id="@+id/action_startApplicationFragment_to_viewUserFragment"
            app:destination="@id/viewUserFragment" />
    </fragment>
    <fragment
        android:id="@+id/startCarFragment"
        android:name="com.example.proftaakandroid.ui.car.StartCarFragment"
