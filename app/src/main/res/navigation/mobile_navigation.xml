            android:id="@+id/action_startCarFragment_to_viewCarFragment"
            app:destination="@id/viewCarFragment" />
    </fragment>
    <fragment
        android:id="@+id/viewCarFragment"
        android:name="com.example.proftaakandroid.ui.car.ViewCarFragment"
        android:label="ViewCarFragment" >
