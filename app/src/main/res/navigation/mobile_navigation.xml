            android:id="@+id/action_startApplicationFragment_to_viewCarFragment"
            app:destination="@id/viewCarFragment" />
        <action
            android:id="@+id/action_startApplicationFragment_to_viewListingFragment"
            app:destination="@id/viewListingFragment" />
